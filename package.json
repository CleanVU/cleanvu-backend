{
  "name": "cleanvu-backend",
  "version": "1.0.0",
  "description": "The backend for the CleanVU application",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon src/index.ts",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "seed": "ts-node src/scripts/seed.ts",
    "typescript:check": "tsc --esModuleInterop",
    "lint:check": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier:format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prepare": "husky install"
  },
  "author": "Jacob Lurie and Seth Marx",
  "license": "ISC",
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.11",
    "@types/jest": "^29.5.11",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http": "^0.0.1-security",
    "mongodb": "^6.3.0",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "pino": "^8.17.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  }
}
